{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "FerriteQL",
  "scopeName": "source.ferriteql",
  "patterns": [
    { "include": "#comments" },
    { "include": "#commands" },
    { "include": "#strings" },
    { "include": "#numbers" },
    { "include": "#keywords" },
    { "include": "#operators" },
    { "include": "#variables" }
  ],
  "repository": {
    "comments": {
      "patterns": [
        {
          "name": "comment.line.double-slash.ferriteql",
          "match": "//.*$"
        },
        {
          "name": "comment.line.hash.ferriteql",
          "match": "#.*$"
        },
        {
          "name": "comment.block.ferriteql",
          "begin": "/\\*",
          "end": "\\*/"
        }
      ]
    },
    "commands": {
      "patterns": [
        {
          "name": "keyword.control.string.ferriteql",
          "match": "\\b(GET|SET|SETNX|SETEX|PSETEX|MGET|MSET|MSETNX|GETSET|GETEX|GETDEL|APPEND|STRLEN|INCR|INCRBY|INCRBYFLOAT|DECR|DECRBY|SETRANGE|GETRANGE)\\b",
          "captures": {
            "1": { "name": "support.function.string.ferriteql" }
          }
        },
        {
          "name": "keyword.control.list.ferriteql",
          "match": "\\b(LPUSH|RPUSH|LPUSHX|RPUSHX|LPOP|RPOP|BLPOP|BRPOP|BRPOPLPUSH|LRANGE|LLEN|LINDEX|LSET|LINSERT|LREM|LTRIM|LPOS|LMOVE|BLMOVE)\\b",
          "captures": {
            "1": { "name": "support.function.list.ferriteql" }
          }
        },
        {
          "name": "keyword.control.hash.ferriteql",
          "match": "\\b(HSET|HGET|HMSET|HMGET|HDEL|HEXISTS|HGETALL|HINCRBY|HINCRBYFLOAT|HKEYS|HVALS|HLEN|HSETNX|HSCAN|HRANDFIELD)\\b",
          "captures": {
            "1": { "name": "support.function.hash.ferriteql" }
          }
        },
        {
          "name": "keyword.control.set.ferriteql",
          "match": "\\b(SADD|SREM|SMEMBERS|SISMEMBER|SMISMEMBER|SCARD|SPOP|SRANDMEMBER|SMOVE|SDIFF|SDIFFSTORE|SINTER|SINTERSTORE|SINTERCARD|SUNION|SUNIONSTORE|SSCAN)\\b",
          "captures": {
            "1": { "name": "support.function.set.ferriteql" }
          }
        },
        {
          "name": "keyword.control.sortedset.ferriteql",
          "match": "\\b(ZADD|ZREM|ZSCORE|ZRANK|ZREVRANK|ZRANGE|ZREVRANGE|ZRANGEBYSCORE|ZREVRANGEBYSCORE|ZRANGEBYLEX|ZREVRANGEBYLEX|ZCARD|ZCOUNT|ZLEXCOUNT|ZINCRBY|ZPOPMIN|ZPOPMAX|BZPOPMIN|BZPOPMAX|ZRANGESTORE|ZUNION|ZUNIONSTORE|ZINTER|ZINTERSTORE|ZINTERCARD|ZDIFF|ZDIFFSTORE|ZMPOP|BZMPOP|ZRANDMEMBER|ZSCAN|ZMSCORE)\\b",
          "captures": {
            "1": { "name": "support.function.sortedset.ferriteql" }
          }
        },
        {
          "name": "keyword.control.stream.ferriteql",
          "match": "\\b(XADD|XREAD|XREADGROUP|XRANGE|XREVRANGE|XLEN|XINFO|XGROUP|XACK|XCLAIM|XAUTOCLAIM|XPENDING|XTRIM|XDEL|XSETID)\\b",
          "captures": {
            "1": { "name": "support.function.stream.ferriteql" }
          }
        },
        {
          "name": "keyword.control.key.ferriteql",
          "match": "\\b(DEL|EXISTS|EXPIRE|EXPIREAT|EXPIRETIME|PEXPIRE|PEXPIREAT|PEXPIRETIME|TTL|PTTL|PERSIST|TYPE|KEYS|SCAN|RENAME|RENAMENX|TOUCH|UNLINK|COPY|DUMP|RESTORE|OBJECT|WAIT|MIGRATE|SORT|SORT_RO)\\b",
          "captures": {
            "1": { "name": "support.function.key.ferriteql" }
          }
        },
        {
          "name": "keyword.control.pubsub.ferriteql",
          "match": "\\b(PUBLISH|SUBSCRIBE|UNSUBSCRIBE|PSUBSCRIBE|PUNSUBSCRIBE|PUBSUB|SSUBSCRIBE|SUNSUBSCRIBE|SPUBLISH)\\b",
          "captures": {
            "1": { "name": "support.function.pubsub.ferriteql" }
          }
        },
        {
          "name": "keyword.control.transaction.ferriteql",
          "match": "\\b(MULTI|EXEC|DISCARD|WATCH|UNWATCH)\\b",
          "captures": {
            "1": { "name": "support.function.transaction.ferriteql" }
          }
        },
        {
          "name": "keyword.control.scripting.ferriteql",
          "match": "\\b(EVAL|EVALSHA|EVALSHA_RO|EVAL_RO|SCRIPT|FUNCTION|FCALL|FCALL_RO)\\b",
          "captures": {
            "1": { "name": "support.function.scripting.ferriteql" }
          }
        },
        {
          "name": "keyword.control.server.ferriteql",
          "match": "\\b(PING|ECHO|INFO|DBSIZE|FLUSHDB|FLUSHALL|SELECT|QUIT|AUTH|CLIENT|CONFIG|DEBUG|MEMORY|TIME|SLOWLOG|COMMAND|ACL|BGSAVE|BGREWRITEAOF|SAVE|LASTSAVE|SHUTDOWN|SLAVEOF|REPLICAOF|ROLE|SENTINEL|CLUSTER|READONLY|READWRITE|FAILOVER|LATENCY|MODULE|SWAPDB|LOLWUT|RESET)\\b",
          "captures": {
            "1": { "name": "support.function.server.ferriteql" }
          }
        },
        {
          "name": "keyword.control.geo.ferriteql",
          "match": "\\b(GEOADD|GEOPOS|GEODIST|GEORADIUS|GEORADIUSBYMEMBER|GEOHASH|GEOSEARCH|GEOSEARCHSTORE)\\b",
          "captures": {
            "1": { "name": "support.function.geo.ferriteql" }
          }
        },
        {
          "name": "keyword.control.hyperloglog.ferriteql",
          "match": "\\b(PFADD|PFCOUNT|PFMERGE|PFDEBUG|PFSELFTEST)\\b",
          "captures": {
            "1": { "name": "support.function.hyperloglog.ferriteql" }
          }
        },
        {
          "name": "keyword.control.bitmap.ferriteql",
          "match": "\\b(SETBIT|GETBIT|BITCOUNT|BITOP|BITPOS|BITFIELD|BITFIELD_RO)\\b",
          "captures": {
            "1": { "name": "support.function.bitmap.ferriteql" }
          }
        },
        {
          "name": "keyword.control.vector.ferriteql",
          "match": "\\b(VECTOR\\.INDEX\\.CREATE|VECTOR\\.INDEX\\.DROP|VECTOR\\.INDEX\\.INFO|VECTOR\\.ADD|VECTOR\\.DEL|VECTOR\\.GET|VECTOR\\.SEARCH|VECTOR\\.MSEARCH)\\b",
          "captures": {
            "1": { "name": "support.function.vector.ferriteql" }
          }
        },
        {
          "name": "keyword.control.semantic.ferriteql",
          "match": "\\b(SEMANTIC\\.SET|SEMANTIC\\.GET|SEMANTIC\\.SEARCH|SEMANTIC\\.DELETE|SEMANTIC\\.INDEX|SEMANTIC\\.CACHE\\.SET|SEMANTIC\\.CACHE\\.GET|SEMANTIC\\.CACHE\\.INVALIDATE)\\b",
          "captures": {
            "1": { "name": "support.function.semantic.ferriteql" }
          }
        },
        {
          "name": "keyword.control.timeseries.ferriteql",
          "match": "\\b(TS\\.CREATE|TS\\.ADD|TS\\.MADD|TS\\.INCRBY|TS\\.DECRBY|TS\\.DEL|TS\\.GET|TS\\.MGET|TS\\.RANGE|TS\\.MRANGE|TS\\.REVRANGE|TS\\.MREVRANGE|TS\\.INFO|TS\\.QUERYINDEX|TS\\.ALTER|TS\\.CREATERULE|TS\\.DELETERULE)\\b",
          "captures": {
            "1": { "name": "support.function.timeseries.ferriteql" }
          }
        },
        {
          "name": "keyword.control.document.ferriteql",
          "match": "\\b(DOC\\.INSERT|DOC\\.GET|DOC\\.UPDATE|DOC\\.DELETE|DOC\\.FIND|DOC\\.AGGREGATE|DOC\\.INDEX\\.CREATE|DOC\\.INDEX\\.DROP|DOC\\.INDEX\\.LIST)\\b",
          "captures": {
            "1": { "name": "support.function.document.ferriteql" }
          }
        },
        {
          "name": "keyword.control.graph.ferriteql",
          "match": "\\b(GRAPH\\.QUERY|GRAPH\\.CREATE|GRAPH\\.DELETE|GRAPH\\.EXPLAIN|GRAPH\\.PROFILE|GRAPH\\.SLOWLOG|GRAPH\\.CONFIG)\\b",
          "captures": {
            "1": { "name": "support.function.graph.ferriteql" }
          }
        },
        {
          "name": "keyword.control.crdt.ferriteql",
          "match": "\\b(CRDT\\.GCOUNTER\\.INCR|CRDT\\.GCOUNTER\\.GET|CRDT\\.PNCOUNTER\\.INCR|CRDT\\.PNCOUNTER\\.DECR|CRDT\\.PNCOUNTER\\.GET|CRDT\\.LWWREG\\.SET|CRDT\\.LWWREG\\.GET|CRDT\\.ORSET\\.ADD|CRDT\\.ORSET\\.REM|CRDT\\.ORSET\\.GET|CRDT\\.MERGE)\\b",
          "captures": {
            "1": { "name": "support.function.crdt.ferriteql" }
          }
        },
        {
          "name": "keyword.control.cluster.ferriteql",
          "match": "\\b(CLUSTER\\s+INFO|CLUSTER\\s+NODES|CLUSTER\\s+MEET|CLUSTER\\s+FORGET|CLUSTER\\s+REPLICATE|CLUSTER\\s+SAVECONFIG|CLUSTER\\s+ADDSLOTS|CLUSTER\\s+DELSLOTS|CLUSTER\\s+FLUSHSLOTS|CLUSTER\\s+SETSLOT|CLUSTER\\s+KEYSLOT|CLUSTER\\s+COUNTKEYSINSLOT|CLUSTER\\s+GETKEYSINSLOT|CLUSTER\\s+FAILOVER|CLUSTER\\s+RESET|CLUSTER\\s+MYID|CLUSTER\\s+SLOTS|CLUSTER\\s+SHARDS|CLUSTER\\s+LINKS)\\b",
          "captures": {
            "1": { "name": "support.function.cluster.ferriteql" }
          }
        }
      ]
    },
    "strings": {
      "patterns": [
        {
          "name": "string.quoted.double.ferriteql",
          "begin": "\"",
          "end": "\"",
          "patterns": [
            {
              "name": "constant.character.escape.ferriteql",
              "match": "\\\\."
            }
          ]
        },
        {
          "name": "string.quoted.single.ferriteql",
          "begin": "'",
          "end": "'",
          "patterns": [
            {
              "name": "constant.character.escape.ferriteql",
              "match": "\\\\."
            }
          ]
        }
      ]
    },
    "numbers": {
      "patterns": [
        {
          "name": "constant.numeric.float.ferriteql",
          "match": "\\b[0-9]+\\.[0-9]+\\b"
        },
        {
          "name": "constant.numeric.integer.ferriteql",
          "match": "\\b[0-9]+\\b"
        },
        {
          "name": "constant.numeric.special.ferriteql",
          "match": "\\b(\\+inf|-inf|inf|NaN)\\b"
        }
      ]
    },
    "keywords": {
      "patterns": [
        {
          "name": "constant.language.ferriteql",
          "match": "\\b(NX|XX|EX|PX|EXAT|PXAT|KEEPTTL|GET|IFEQ|IFGT|IFLT|GT|LT|CH|INCR|WITHSCORES|LIMIT|MATCH|COUNT|TYPE|NOSORT|ASC|DESC|ALPHA|BY|STORE|AGGREGATE|SUM|MIN|MAX|WEIGHTS|KEYS|ARGV|COPY|REPLACE|ABSTTL|IDLETIME|FREQ|BEFORE|AFTER|LEFT|RIGHT|BLOCK|NOACK|STREAMS|GROUP|CREATE|DESTROY|SETID|CONSUMERS|CREATECONSUMER|DELCONSUMER|MKSTREAM|NOMKSTREAM|MINID|MAXLEN|WITHVALUES|REV|BYLEX|BYSCORE)\\b"
        },
        {
          "name": "constant.language.boolean.ferriteql",
          "match": "\\b(true|false|nil|null|OK|QUEUED|PONG)\\b"
        }
      ]
    },
    "operators": {
      "patterns": [
        {
          "name": "keyword.operator.ferriteql",
          "match": "(==|!=|<=|>=|<|>|&&|\\|\\||!|\\+|-|\\*|/|%)"
        }
      ]
    },
    "variables": {
      "patterns": [
        {
          "name": "variable.other.key.ferriteql",
          "match": "\\b[a-zA-Z_][a-zA-Z0-9_:.-]*\\b"
        }
      ]
    }
  }
}
